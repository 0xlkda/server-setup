{
  "name": "server-setup",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon"
  },
  "keywords": [],
  "author": "Alex <lekhacduyanh@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "chokidar": "^3.5.3",
    "dotenv": "^16.1.4",
    "express": "4.18.2",
    "express-session": "^1.17.3",
    "lodash": "^4.17.21",
    "lowdb": "^6.0.1",
    "multer": "1.4.5-lts.1",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "eslint": "^8.42.0",
    "nodemon": "^2.0.22"
  },
  "nodemonConfig": {
    "env": {
      "NODE_ENV": "development",
      "SESSION_NAME": "session_name",
      "SESSION_SECRET": "session_secret"
    },
    "watch": [
      "./**/*.js",
      ".env"
    ],
    "ignore": [
      ".git",
      "*.json",
      "node_modules/**/node_modules"
    ]
  }
}
